{
    "name": "ConEd/ORU MQTT Bridge",
    "version": "0.3.0",
    "slug": "coned",
    "description": "Read your ConEd/ORU meter energy usage into your Home Assistant via MQTT",
    "url": "https://github.com/jpoles1/home-assistant-coned",
    "panel_icon": "mdi:chart-areaspline",
    "startup": "application",
    "homeassistant": "0.92.0b2",
    "boot": "auto",
    "map": ["share:rw", "ssl"],
    "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
    "host_network": "False",
    "arch": [
      "aarch64",
      "amd64",
      "armv7",
      "i386"
    ],
    "hassio_api": true,
    "auth_api": true,
    "hassio_role": "default",
    "homeassistant_api": false,
    "options":
    {
      "email": "",
      "password": "",
      "mfa_type": "",
      "mfa_secret": "",
      "account_uuid": "",
      "meter_number": "",
      "site": "",
      "mqtt_host": "hassio.local",
      "mqtt_user": "",
      "mqtt_password": ""
    },
    "schema":
    {
      "email": "str",
      "password": "str",
      "mfa_type": "str",
      "mfa_secret": "str",
      "account_uuid": "str",
      "meter_number": "str",
      "site": "str",
      "mqtt_host": "str",
      "mqtt_user": "str",
      "mqtt_password": "str"
     }
  }
  